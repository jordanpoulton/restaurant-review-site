
<h1> Welcome to the Restaurant Review Site </h1>

<p><h2> Current Restaurants:</h2></p>


<table style='border: 2px solid #999'>
  <thead>
    <th> Restaurant Name </th>
  </thead>
  <tbody>
    <% @restaurants.each do |restaurant| %>
    <tr>
      <td> <%= link_to restaurant.name, restaurant_path(restaurant) %>
    </tr>
    <%end%>
  </tbody>
</table>

<p><%= link_to "Create new Restaurant", new_restaurant_path %></p>

